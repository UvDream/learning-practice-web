{
  "presets": [
    [
      "@babel/preset-env",
      {
        // 将es6的语法翻译成es5语法
        "targets": {
          "chrome": "67"
        },
        "useBuiltIns": "usage", // 做@babel/polyfill补充时，按需补充，用到什么才补充什么,
        "corejs": "2"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"]
}
